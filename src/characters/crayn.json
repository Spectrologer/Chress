  "interaction": {
    "type": "dialogue",
    "cycleMode": "sequential",
    "dialogueTree": [
      {
        "id": "msg_0",
        "text": "You dropped your axe down the well.",
        "buttonText": "Oh..."
      },
      {
        "id": "msg_1",
        "text": "Point at places you want to go.",
        "buttonText": "Got it."
      },
      {
        "id": "msg_2",
        "text": "Sometimes I chop a tree to pass the time.",
        "buttonText": "Cool."
      },
      {
        "id": "msg_3",
        "text": "They say you are descended from a King.",
        "buttonText": "Really?"
      },
      {
        "id": "msg_4",
        "text": "The Museum is members only.",
        "buttonText": "Okay..."
      },
      {
        "id": "msg_5",
        "text": "Spears can be used to get past obstacles.",
        "buttonText": "Thanks."
      },
      {
        "id": "msg_6",
        "text": "Bombs can send you flying.",
        "buttonText": "Whoa."
      },
      {
        "id": "msg_7",
        "text": "You can use as many items as you like.",
        "buttonText": "Good to know."
      },
      {
        "id": "msg_8",
        "text": "The path gets harder the farther you go.",
        "buttonText": "I see."
      },
      {
        "id": "msg_9",
        "text": "Sometimes fighting is a bad choice.",
        "buttonText": "True."
      },
      {
        "id": "msg_10",
        "text": "There is no end to the frontier.",
        "buttonText": "Understood."
      }
    ]
  },
  "behavior": {
    "movementType": "stationary",
    "walkable": false
  },
